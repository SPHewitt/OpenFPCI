FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Large Strain
solidSolver femLargeStrain;

femLargeStrainCoeffs
{

    nonLinear off;   // Not required for ParaFEM
  
    moveMesh yes;

//    gravity (0 0 0);
    gravity 0.0;

    beta 0.4;
    delta 0.6;

    PCGTolerance    1e-6;
    PCGLimit        500;
    NRTolerance     1e-5;

    rbfUpdate false;
}






