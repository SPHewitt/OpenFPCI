FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Small Strain
//solidSolver femSmallStrain;

// Large Strain
solidSolver femLargeStrain;

femSmallStrainCoeffs
{

    nonLinear off;   // Not required for ParaFEM
  
    moveMesh yes;

    gravity 0.0;

    alpha1 0;
    beta1 0;
    theta 1.0;

    rbfUpdate false;

    timeStep 0.001;
}

femLargeStrainCoeffs
{

    nonLinear off;   // Not required for ParaFEM

    moveMesh yes;

    gravity 0.0;

    beta 0.4;
    delta 0.6;

    rbfUpdate false;

}






